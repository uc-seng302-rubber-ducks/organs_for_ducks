<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="735.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="odms.controller.gui.panel.view.BloodTestViewController">
   <children>
      <TableView fx:id="bloodTestView" layoutX="48.0" layoutY="66.0" prefHeight="610.0" prefWidth="701.0">
        <columns>
            <TableColumn fx:id="testDateColumn" prefWidth="140.0" text="Date Requested"/>
            <TableColumn fx:id="lowPropertyValuesColumn" prefWidth="125.0" text="Properties Below Normal"/>
            <TableColumn fx:id="highPropertyValuesColumn" prefWidth="137.0" text="Properties Above Normal"/>
        </columns>
          <columnResizePolicy>
              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
          </columnResizePolicy>
      </TableView>
      <HBox alignment="CENTER" layoutX="150.0" layoutY="693.0" prefHeight="25.0" prefWidth="492.0">
         <children>
            <Button layoutX="322.0" layoutY="465.0" mnemonicParsing="false" onAction="#goToPreviousPage" text="&lt;&lt;" />
            <Button layoutX="362.0" layoutY="465.0" mnemonicParsing="false" onAction="#goToNextPage" text="&gt;&gt;" />
         </children>
      </HBox>
      <Label layoutX="46.0" layoutY="35.0" prefHeight="24.0" prefWidth="355.0" text="Blood Tests">
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Label>
      <Button fx:id="requestNewBloodTest" layoutX="600.0" layoutY="33.0" mnemonicParsing="false" onAction="#addNewBloodTest" text="Add New Blood Test" />
      <GridPane layoutX="769.0" layoutY="99.0" prefHeight="549.0" prefWidth="369.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Red Blood Cell Count:" />
            <Label text="White Blood Cell Count:" GridPane.rowIndex="1" />
            <Label text="Heamoglobin Level:" GridPane.rowIndex="2" />
            <Label text="Platelets:" GridPane.rowIndex="3" />
            <Label text="Glucose Levels:" GridPane.rowIndex="4" />
            <Label text="Mean Cell Volume:" GridPane.rowIndex="5" />
            <Label text="Haematocrit:" GridPane.rowIndex="6" />
            <Label text="Mean Cell Haematocrit:" GridPane.rowIndex="7" />
            <TextField fx:id="redBloodCount" GridPane.columnIndex="1" />
            <TextField fx:id="whiteBloodCount" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="heamoglobin" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField fx:id="platelets" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <TextField fx:id="glucose" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <TextField fx:id="meanCellVolume" GridPane.columnIndex="1" GridPane.rowIndex="5" />
            <TextField fx:id="haematocrit" GridPane.columnIndex="1" GridPane.rowIndex="6" />
            <TextField fx:id="meanCellHaematocrit" GridPane.columnIndex="1" GridPane.rowIndex="7" />
         </children>
      </GridPane>
      <Button fx:id="deleteBloodTest" layoutX="765.0" layoutY="655.0" mnemonicParsing="false" text="Delete Blood test" />
      <Button fx:id="updateBloodTest" layoutX="998.0" layoutY="655.0" mnemonicParsing="false" text="Update Blood Test" />
      <Label layoutX="767.0" layoutY="73.0" text="Blood Test Details">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
