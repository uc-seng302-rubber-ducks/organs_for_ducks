<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@../gui.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="odms.controller.gui.panel.MedicationTabController">
    <children>
        <ListView fx:id="currentMedicationListView" layoutX="431.0" layoutY="104.0" onMouseClicked="#clearPreviousMedSelection" prefHeight="287.0" prefWidth="227.0" />
        <ListView fx:id="previousMedicationListView" layoutX="22.0" layoutY="102.0" onMouseClicked="#clearCurrentMedSelection" prefHeight="285.0" prefWidth="221.0" />
        <Button fx:id="untakeMedicationButton" layoutX="315.0" layoutY="200.0" mnemonicParsing="false" onAction="#untakeMedication" text="&lt;&lt;" />
        <Button fx:id="takeMedicationButton" layoutX="314.0" layoutY="262.0" mnemonicParsing="false" onAction="#takeMedication" text="&gt;&gt;" />
        <Button fx:id="deleteMedicationButton" layoutX="70.0" layoutY="448.0" mnemonicParsing="false" onAction="#deleteMedication" text="Delete" />
        <TextField fx:id="medicationTextField" layoutX="425.0" layoutY="49.0" />
        <Button fx:id="addMedicationButton" layoutX="621.0" layoutY="49.0" mnemonicParsing="false" onAction="#addMedication" text="add" />
        <Label layoutX="46.0" layoutY="84.0" styleClass="label-sub-title" text="Previous Medications" />
        <Label layoutX="468.0" layoutY="84.0" styleClass="label-sub-title" text="Current Medications" />
        <Label layoutX="345.0" layoutY="54.0" styleClass="label-title" text="Medication" />
        <Label fx:id="drugDetailsLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="671.0" layoutY="104.0" prefHeight="17.0" prefWidth="198.0" styleClass="label-sub-title" text="Drug Details" textAlignment="CENTER" />
        <TextArea fx:id="drugDetailsTextArea" editable="false" layoutX="670.0" layoutY="125.0" prefHeight="200.0" prefWidth="200.0" wrapText="true" />
    </children>
</AnchorPane>
