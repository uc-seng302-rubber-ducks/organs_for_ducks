<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="735.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8"
            fx:controller="odms.controller.gui.panel.view.UserAppointmentViewController">
    <children>
        <AnchorPane layoutX="455.0" prefHeight="735.0" prefWidth="332.0" AnchorPane.bottomAnchor="0.0"
                    AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <Button fx:id="userCancelAppointmentButton" layoutX="9.0" layoutY="668.0" mnemonicParsing="false"
                        onAction="#cancelAppointment" prefHeight="25.0" prefWidth="152.0" text="Cancel Appointment"
                        AnchorPane.bottomAnchor="50.0" AnchorPane.rightAnchor="171.0"/>
                <Label layoutX="11.0" layoutY="56.0" text="Appointment Details" AnchorPane.rightAnchor="158.0">
                    <font>
                        <Font name="System Bold" size="14.0"/>
                    </font>
                </Label>
                <Label alignment="TOP_LEFT" layoutX="14.0" layoutY="103.0" prefHeight="271.0" prefWidth="117.0"
                       text="Status:&#10;&#10;&#10;Date:&#10;&#10;&#10;Time:&#10;&#10;&#10;Clinician Id:&#10;&#10;&#10;Category:&#10;&#10;&#10;Description:"
                       wrapText="true">
                    <font>
                        <Font name="System Bold" size="13.0"/>
                    </font>
                </Label>
                <Label fx:id="userAppointmentDetailsLabel" alignment="TOP_LEFT" layoutX="131.0" layoutY="103.0"
                       prefHeight="545.0" prefWidth="188.0" wrapText="true"/>
            </children>
        </AnchorPane>
        <AnchorPane prefHeight="735.0" prefWidth="859.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                    AnchorPane.rightAnchor="341.0" AnchorPane.topAnchor="0.0">
            <children>
                <Button fx:id="requestAppointmentButton" layoutX="256.0" layoutY="17.0" mnemonicParsing="false"
                        onAction="#requestNewAppointment" prefHeight="25.0" prefWidth="194.0"
                        text="Request New Appointment" AnchorPane.rightAnchor="6.0" AnchorPane.topAnchor="17.0"/>
                <TableView fx:id="userAppointmentsTableView" prefHeight="634.0" prefWidth="845.0"
                           AnchorPane.bottomAnchor="49.0" AnchorPane.leftAnchor="8.0" AnchorPane.rightAnchor="6.0"
                           AnchorPane.topAnchor="52.0">
                    <columns>
                        <TableColumn fx:id="userAppointmentDateColumn" maxWidth="3000.0" prefWidth="103.0"
                                     text="Date/Time"/>
                        <TableColumn fx:id="userAppointmentClinicianIdColumn" prefWidth="99.0" text="Clinician"/>
                        <TableColumn fx:id="userAppointmentCategoryColumn" maxWidth="4500.0" prefWidth="88.0"
                                     text="Category"/>
                        <TableColumn fx:id="userAppointmentStatusColumn" maxWidth="4000.0" prefWidth="102.0"
                                     text="Status"/>
                    </columns>
                    <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                    </columnResizePolicy>
                </TableView>
                <HBox alignment="CENTER" layoutX="191.0" layoutY="696.0" prefHeight="25.0" prefWidth="492.0"
                      AnchorPane.bottomAnchor="14.0" AnchorPane.leftAnchor="191.0" AnchorPane.rightAnchor="176.0">
                    <children>
                        <Button layoutX="322.0" layoutY="465.0" mnemonicParsing="false" onAction="#goToPreviousPage"
                                text="&lt;&lt;"/>
                        <Button layoutX="362.0" layoutY="465.0" mnemonicParsing="false" onAction="#goToNextPage"
                                text="&gt;&gt;"/>
                    </children>
                </HBox>
            </children>
        </AnchorPane>
        <Label prefHeight="24.0" prefWidth="355.0" text="Appointments" AnchorPane.leftAnchor="14.0"
               AnchorPane.topAnchor="17.0">
            <font>
                <Font name="System Bold" size="19.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
