<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="487.0" prefWidth="670.0" xmlns="http://javafx.com/javafx/8"
            fx:controller="odms.controller.gui.panel.view.UserAppointmentViewController">
    <children>
        <Label layoutX="14.0" layoutY="17.0" prefHeight="24.0" prefWidth="355.0" text="Appointments"
               AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="17.0">
            <font>
                <Font name="System Bold" size="19.0"/>
            </font>
        </Label>
        <TableView fx:id="userAppointmentsTableView" layoutY="60.0" prefHeight="419.0" prefWidth="653.0"
                   AnchorPane.bottomAnchor="8.0" AnchorPane.leftAnchor="8.0" AnchorPane.rightAnchor="9.0"
                   AnchorPane.topAnchor="60.0">
            <columns>
                <TableColumn fx:id="userAppointmentDateColumn" maxWidth="3000.0" prefWidth="89.0" text="Date/Time"/>
                <TableColumn fx:id="userAppointmentClinicianIdColumn" prefWidth="76.0" text="Clinician"/>
                <TableColumn fx:id="userAppointmentCategoryColumn" maxWidth="4500.0" prefWidth="88.0" text="Category"/>
                <TableColumn fx:id="userAppointmentStatusColumn" maxWidth="4000.0" prefWidth="102.0" text="Status"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
        <Button fx:id="requestAppointmentButton" layoutX="468.0" layoutY="12.0" mnemonicParsing="false"
                onAction="#openAppointmentPicker" text="Request New Appointment" AnchorPane.rightAnchor="9.0"/>
    </children>
</AnchorPane>
