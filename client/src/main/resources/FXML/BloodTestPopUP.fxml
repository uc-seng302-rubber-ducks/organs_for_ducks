             <TextField fx:id="meanCellVolume" GridPane.columnIndex="1" GridPane.rowIndex="7"/>
            <TextField fx:id="meanCellHaematocrit" GridPane.columnIndex="1" GridPane.rowIndex="8" />
            <Label text="Test Date:" />
