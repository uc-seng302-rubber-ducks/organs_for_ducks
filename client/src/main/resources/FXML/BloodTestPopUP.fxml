             <Label fx:id="meanCellHaematocritError" prefHeight="15.0" prefWidth="336.0"
                    text="Please enter a postive number" GridPane.columnIndex="1" GridPane.rowIndex="8"
                    GridPane.valignment="BOTTOM"/>
